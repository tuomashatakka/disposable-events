  },
  "url": "https://github.com/tuomashatakka/disposable-events",
  "repository": "https://github.com/tuomashatakka/disposable-events",
  "dependencies": {
    "@types/events": "^3.0.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "ava": "^2.4.0",
    "nyc": "^14.1.1",
    "tap-xunit": "^2.4.1"
  }
}
